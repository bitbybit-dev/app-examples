{
  "name": "express-app",
  "version": "1.0.0",
  "description": "Example of how to use bitbybit-occt within express app",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon --experimental-specifier-resolution=node -q dist/index.js\""
  },
  "author": "Bit By Bit Developers",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "@bitbybit-dev/occt": "0.14.3"
  },
  "type":"module",
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.4"
  }
}
